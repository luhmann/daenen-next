  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luhmann/daenen-next/issues"
  },
  "homepage": "https://github.com/luhmann/daenen-next#readme",
  "dependencies": {
    "next": "^9.0.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-obfuscate": "^3.6.2",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.2",
    "@types/recompose": "^0.30.7",
    "http-server": "^0.11.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.4.4",
    "prop-types": "^15.6.0",
    "short-hash": "^1.0.0",
    "testcafe": "^1.5.0",
    "testcafe-browser-provider-nightmare": "0.0.5",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "tslint --fix --project tsconfig.json",
